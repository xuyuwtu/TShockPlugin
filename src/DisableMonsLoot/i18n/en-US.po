msgid "/kdm reset —— 恢复所有清理开关\n"
msgstr "/kdm reset —— Restore all cleaning switches\n"

#: ../../Commands.cs:21
msgid "/kdm 或 /禁掉落 —— 查看禁怪掉落指令菜单\n"
msgstr "/kdm —— View ban monster drop instruction menu\n"

#: ../../Commands.cs:26
msgid "/reload —— 重载禁怪掉落配置文件\n"
msgstr "/reload —— Reload prevent monster drop configuration file\n"

